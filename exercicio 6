from random import randint
print("="*50)
print("Jogo de adivinhação de numeros entre 1 a 5")
print("="*50)
jogador = int(input("Tente Adivinhar o numero sorteado: "))
random = randint(1,5)

if (jogador==random):
        print("VOCÊ VENCEU!!")
elif(jogador<random):
        print("Valor baixo")
elif(jogador>random):   
        print("Valor alto")
elif():
         print("Tente novamente")
